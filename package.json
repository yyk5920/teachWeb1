    "vue-router": "^3.0.1",
    "vuex": "^3.4.0",
    "wavedrom": "^2.6.8",
    "xe-utils": "^2.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@vuepress/plugin-nprogress": "^1.5.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
